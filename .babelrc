{
    "presets": [
        [
            "env", {
                "modules": false
            }
        ],
        "vue"
    ],
    "plugins": [
        "transform-custom-element-classes",
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true
            }
        ]
    ],
    "ignore": [
        "dist/*.js"
    ]
}
