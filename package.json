{
  "name": "mip",
  "version": "2.0.0",
  "description": "mobile instant page",
  "main": "dist/mip.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "node examples/server.js",
    "build": "node build/build.js",
    "dev:server": "node examples/server.rollup.js",
    "dev:rollup": "rollup -w -c build/rollup.config.js --environment TARGET:web-full-dev",
    "build:rollup": "rollup -c build/rollup.config.js --environment TARGET:web-full-prod",
    "build:router:rollup": "rollup -c build/rollup.config.js --environment TARGET:router",
    "build:store:rollup": "rollup -c build/rollup.config.js --environment TARGET:store"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mip-project/mip.git"
  },
